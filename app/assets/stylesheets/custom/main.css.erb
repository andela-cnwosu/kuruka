/*************************
*******Header******
**************************/


#header{
  margin-bottom: 0;
  padding: 30px 0 50px 0;
}

#header .navbar-inverse .container{
  position: relative;
}

.search{
  position: absolute;
  top: 20px;
  width: 100%;
  right: 0;
  z-index: 1;
}

.search i{
  font-size: 14px;
  color: #00aeef;
  cursor: pointer;
  position: absolute;
  right: 10px;
  top: 2px;
}

.field-toggle{
  position: relative;
  top: 30px;
  right: 15px;
  display: none;
  height: 50px;
}

#header .navbar {
  background: #fff;
  border: 0;
  margin-bottom: 0;
}

#header .navbar-toggle{
  margin-top: 20px;
}

#header .navbar-brand{
  padding: 0;
  margin-left: 0;
}

#header .navbar-brand h1{
  padding: 0;
  margin: 0;
}

#header .navbar-brand img{
  height: 50px;
}

#header .navbar-nav.navbar-right >li:last-child{
  margin-right: -10px;
}

#header .navbar-nav.navbar-right >.nav-link a {
  cursor: pointer;
  color: #000;
  font-size: 12px;
  padding: 20px;
  text-transform: uppercase;
  font-weight: 400;
}

#header .navbar-nav.navbar-right >.nav-link .dropdown-menu {
  border: none;
}

.dropdown-menu {
  right: 0;
  left: inherit;
}

#header .dropdown-menu .media-heading {
  margin-top: 5px;
}

#header .navbar-nav.navbar-right >.nav-link #profile-menu {
  margin-top: -5px;
  padding-right: 15px;
}

#header .navbar-nav.navbar-right >.nav-link .dropdown-menu>li>a, 
#header .nav-link .dropdown-menu>li>a {
  padding: 0;
}

#header .navbar-nav.navbar-right >.nav-link .dropdown-menu>li, 
#header .nav-link .dropdown-menu>li {
  padding: 10px 15px;
}

#header .navbar-nav.navbar-right >.nav-link .dropdown-menu>li.divider, 
#header .nav-link .dropdown-menu>li.divider {
  padding: 0;
}

#header .navbar-inverse .navbar-toggle.navbar-user,
#header .navbar-inverse .navbar-toggle.navbar-user:hover,
#header .navbar-inverse .navbar-toggle.navbar-user:focus {
    background: none;
    margin-top: 10px;
}

#header .navbar-inverse .navbar-nav  li.active > a,  
#header .navbar-inverse .navbar-nav  li.active > a:focus, 
#header .navbar-nav.navbar-right li > a:hover,
.navbar-inverse .navbar-nav > .open > a {
  background-color: inherit;
  border: 0;
  color: #0f93db;
}

#header .navbar-inverse .navbar-nav  li a:hover{
  color: #00aeef;
}

#header .navbar-nav.navbar-right >.nav-link .dropdown-menu .media-body {
  h5, h6 {
    font-weight: 400;
  }
}

.heading-caption {
  text-align: center;
  margin-bottom: 40px;
  margin-top: -20px;
}

.fa-angle-down{
  padding-left: 5px; 
}

.scaleIn {
  -webkit-animation-name: scaleIn;
  animation-name: scaleIn;
}

@-webkit-keyframes scaleIn {
  0% {
    opacity: 0;
    -webkit-transform: scale(0);
    transform: scale(0);
  }

  100% {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}

@keyframes scaleIn {
  0% {
    opacity: 0;
    -webkit-transform: scale(0);
    -ms-transform: scale(0);
    transform: scale(0);
  }

  100% {
    opacity: 1;
    -webkit-transform: scale(1);
    -ms-transform: scale(1);
    transform: scale(1);
  }
}


